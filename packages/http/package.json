{
  "name": "@shware/http",
  "version": "1.1.2",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsup",
    "test": "jest",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShwareHQ/shware-sdk.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./polyfills": {
      "types": "./dist/polyfills/index.d.ts",
      "import": "./dist/polyfills/index.mjs",
      "require": "./dist/polyfills/index.cjs"
    },
    "./hono": {
      "types": "./dist/hono/index.d.ts",
      "import": "./dist/hono/index.mjs",
      "require": "./dist/hono/index.cjs"
    },
    "./webhook": {
      "types": "./dist/webhook/index.d.ts",
      "import": "./dist/webhook/index.mjs",
      "require": "./dist/webhook/index.cjs"
    },
    "./google-one-tap": {
      "types": "./dist/google-one-tap/index.d.ts",
      "import": "./dist/google-one-tap/index.mjs",
      "require": "./dist/google-one-tap/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "zod": "^3.25.74"
  },
  "peerDependencies": {
    "axios": "^1.10.0",
    "hono": "^4.7.9",
    "i18next": "^25.0.0",
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "i18next": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
