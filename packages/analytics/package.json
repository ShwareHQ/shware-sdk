{
  "name": "@shware/analytics",
  "version": "0.1.6",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsup"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./web": {
      "types": "./dist/web/index.d.ts",
      "import": "./dist/web/index.js",
      "require": "./dist/web/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.8.3",
    "axios-retry": "^4.5.0",
    "bowser": "^2.11.0",
    "cookie": "^1.0.2",
    "limiter": "^3.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  }
}
